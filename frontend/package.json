{
  "name": "udoco",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.19.1",
    "npm-watch": "^0.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14"
  },
  "watch": {
    "integrate": "src/*.{js,css}"
  },
  "scripts": {
    "watch": "npm-watch",
    "integrate": "react-scripts build && cp build/static/js/main.*.js ../static/app.js && cp build/static/css/main.*.css ../static/app.css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
